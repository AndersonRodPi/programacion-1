Algoritmo Ejercicio_4
	//Calcular el salario semanal de un empleado
	Escribir "Ingrese la cantidad de días que trabaja a la semana: ";
	Leer dias;
	Escribir "Ingrese el salario que recibe al día: ";
	Leer salario_diario;
	salario_semanal <- dias*salario_diario;
	Escribir "El salario semanal del empleado es: ", salario_semanal;
FinAlgoritmo
